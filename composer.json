{
    "name": "mindplay/testies",
    "description": "Yeah, testies: a lightweight library for quick, simple unit-testing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rasmus Schultz",
            "email": "rasc@fynskemedier.dk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://bitbucket.org/mindplaydk/test-interop.git"
        }
    ],
    "require": {
        "php": ">= 7.1",
        "test-interop/test-listener": "1.0.0.x-dev"
    },
    "require-dev": {
        "phpunit/php-code-coverage": "^6.1.1",
        "guzzlehttp/guzzle": "^6.3.3"
    },
    "suggest": {
        "phpunit/php-code-coverage": "^6.1.1"
    },
    "autoload": {
        "files": ["src/test.func.php"],
        "psr-4": {
            "mindplay\\testies\\": "src/"
        }
    }
}
